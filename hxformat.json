{
	"whitespace": {
		"typeCheckColonPolicy": "onlyAfter",
		"typeHintColonPolicy": "onlyAfter"
	},
	"emptyLines": {
		"afterBlocks": "keep",
		"beforeBlocks": "keep",
		"afterLeftCurly": "keep",
		"beforeRightCurly": "keep",
		"classEmptyLines": {
			"afterPrivateVars": 0,
			"afterStaticVars": 0
		},
		"importAndUsing": {
			"betweenImports": 1,
			"betweenImportsLevel": "firstLevelPackage"
		}
	}
}